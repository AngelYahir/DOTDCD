<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    Agregar Cliente <span class="fw-300"><i></i></span>
                </h2>
                <div class="panel-toolbar">
                    <button class="btn btn-panel" data-action="panel-collapse" data-toggle="tooltip" data-offset="0,10"
                        data-original-title="Collapse"></button>
                    <button class="btn btn-panel" data-action="panel-fullscreen" data-toggle="tooltip"
                        data-offset="0,10" data-original-title="Fullscreen"></button>
                    <button class="btn btn-panel" data-action="panel-close" data-toggle="tooltip" data-offset="0,10"
                        data-original-title="Close"></button>
                </div>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <div id="metasAll">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-3"></div>
                                <div class="col-md-6">
                                    <form method="post"class="form-group">
                                        <label class="form-label" for="simpleinput">
                                            <h3>Agregar Cliente</h3>
                                        </label><br>
                                        <span>Razón Social</span>
                                        <input type="text" id="simpleinput" class="form-control mb-1 display-4"
                                            placeholder="Ingresa la razon social" autofocus> <br>
                                        <span>Calle</span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa la calle y número"> <br>
                                        <span>Colonia</span>
                                        <input type="text" id="simpleinput" class="form-control mb-1 "
                                            placeholder="Ingresa la colonia"> <br>
                                        <span> Municipio </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el municipio"> <br>
                                        <span>Estado</span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el estado "> <br>
                                        <span>Codigo Postal </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el código postal "> <br>
                                        <span> RFC </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el RFC"> <br>
                                        <span> Teléfono </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el telefono"> <br>
                                        <span> Contacto </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el contacto"> <br>
                                        <span> Contacto de Cobranza </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el contacto de cobranza"><br>
                                        <span> Correo de Cliente </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el correo del cliente"> <br>
                                        
                                        <span> Régimen Fiscal </span>
                                        <input type="text" id="simpleinput" class="form-control mb-1"
                                            placeholder="Ingresa el régimen fiscal"> <br>
                                        <br><br>
                                        <button type="button"
                                            class="btn-outline-info btn-lg btn-block ">Agregar</button>
                                    </form>
                                    <div class="col-md-3"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>